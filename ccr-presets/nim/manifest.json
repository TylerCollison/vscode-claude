{
  "name": "nim",
  "version": "1.0.0",
  "description": "Full collection of all Nvidia NIM models",
  "author": "Tyler Collison",
  "keywords": [
    "nim"
  ],
  "LOG": false,
  "LOG_LEVEL": "debug",
  "CLAUDE_PATH": "",
  "HOST": "0.0.0.0",
  "PORT": 3456,
  "NIM_API_KEY": "${NIM_API_KEY}",
  "API_TIMEOUT_MS": "600000",
  "PROXY_URL": "",
  "transformers": [],
  "Providers": [
    {
      "name": "nim",
      "api_base_url": "https://integrate.api.nvidia.com/v1/chat/completions",
      "api_key": "${NIM_API_KEY}",
      "models": [
        "deepseek-ai/deepseek-r1-0528",
        "deepseek-ai/deepseek-v3.2",
        "deepseek-ai/deepseek-v3.1-terminus",
        "deepseek-ai/deepseek-v3.1",
        "moonshotai/kimi-k2-thinking",
        "moonshotai/kimi-k2-instruct",
        "meta/llama-3.2-90b-vision-instruct",
        "google/gemma-3-27b-it",
        "mistralai/devstral-2-123b-instruct-2512",
        "mistralai/ministral-14b-instruct-2512",
        "microsoft/phi-3.5-vision-instruct",
        "microsoft/phi-4-multimodal-instruct",
        "meta/llama-4-maverick-17b-128e-instruct",
        "mistralai/mistral-large-3-675b-instruct-2512",
        "nvidia/nemotron-nano-12b-v2-vl",
        "nvidia/cosmos-reason2-8b"
      ]
    }
  ],
  "StatusLine": {
    "enabled": false,
    "currentStyle": "default",
    "default": {
      "modules": []
    },
    "powerline": {
      "modules": []
    }
  },
  "Router": {
    "default": "nim,deepseek-ai/deepseek-v3.1-terminus",
    "background": "nim,deepseek-ai/deepseek-v3.1-terminus",
    "think": "nim,deepseek-ai/deepseek-v3.1-terminus",
    "longContext": "nim,deepseek-ai/deepseek-v3.1-terminus",
    "longContextThreshold": 60000,
    "webSearch": "nim,deepseek-ai/deepseek-v3.1-terminus",
    "image": "nim,nvidia/nemotron-nano-12b-v2-vl"
  },
  "CUSTOM_ROUTER_PATH": ""
}